{
  "name": "arraypress/wp-status-badge",
  "version": "1.0.0",
  "description": "A lightweight WordPress library for rendering styled status badges",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-status-badge/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "arraypress/wp-composer-assets": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\StatusBadge\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-status-badge/issues",
    "source": "https://github.com/arraypress/wp-status-badge"
  },
  "keywords": [
    "wordpress",
    "status",
    "badge",
    "label",
    "admin",
    "frontend",
    "ui",
    "arraypress"
  ]
}